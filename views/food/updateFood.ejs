<!DOCTYPE html>
<html>
  <head>
    <%- include('../partials/header', {title: 'food'}); %>
  </head>
  <body>
    <% include ../partials/navbar %>
    <div class="container">
        <h1>Update Food</h1>
     
        <form method="POST" action="/settings/food/update">
          <input type="hidden" name="id" value="<%=data._id%>" />
         <div class="form-group">
           <label for="Food">Food Name</label>
           <input type="text" class="form-control" id="Food" placeholder="Enter Food Name" name="Food" value="<%= data.name %>">
         </div>
         <button type="submit" class="btn btn-primary">Submit</button>
       </form>
      
       </div>

    <% include ../partials/scripts %>
  </body>
</html>