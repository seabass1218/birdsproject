<!DOCTYPE html>
<html>
<%- include('../partials/header', {title: 'Feedings'}); %>

<body>
    <% include ../partials/navbar %>

    <div class="container">
        <form action="/feedings/export/submit" class="col-12">
            <h1>Export Feedings</h1>

            <div class="form-group">
                <label for="startDate">Start Date</label>
                <input type="date" required class="form-control" id="startDate" placeholder="Enter start date"
                    name="startDate">
            </div>

            <div class="form-group">
                <label for="endDate">End Date</label>
                <input type="date" required class="form-control" id="endDate" placeholder="Enter end date"
                    name="endDate">
            </div>
            <button type="submit" class="btn btn-primary">Export</button>
        </form>

        <% include ../partials/scripts %>
</body>

</html>